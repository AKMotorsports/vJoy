TARGETNAME=hidkmdf
TARGETTYPE=DRIVER

KMDF_VERSION_MAJOR=1
# Support for older versions of KMDF
KMDF_VERSION=1




TARGETLIBS=$(DDK_LIB_PATH)\hidclass.lib
C_DEFINES=$(C_DEFINES) -DBUILD_ALT_DIR=\"$(BUILD_ALT_DIR)\"


SOURCES= hidkmdf.c \
        hidkmdf.rc

TARGET_DESTINATION=wdf

#!IF "$(DDK_TARGET_OS)"!="Win7"
INSTALL_DIR=..\..\install
NTTARGETFILE2= $(INSTALL_DIR)\$(O)\$(TARGETNAME).sys
#!ENDIF
